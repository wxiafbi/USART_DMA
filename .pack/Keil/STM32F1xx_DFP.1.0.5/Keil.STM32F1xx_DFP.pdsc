  <components>
    <component Cclass="Device" Cgroup="Startup" Cversion="1.0.0" condition="STM32F1xx CMSIS Device">  <!-- Cversion is necessary -->
      <description>System Startup for STMicroelectronics STM32F1xx device series</description>
      <files>
        <!--  include folder -->
        <file category="include" name="Device\Include\"/>

        <!-- Flash Option Bytes templates -->
        <file category="source" name="Device\Source\ARM\STM32F1xx_OPT.s"           attr="template"  select="Flash Option Bytes Template"/>

        <!-- startup files -->
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_ld_vl.s" attr="config"    condition="STM32F1xx VL LD"/>
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_md_vl.s" attr="config"    condition="STM32F1xx VL MD"/>
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_hd_vl.s" attr="config"    condition="STM32F1xx VL HD"/>
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_ld.s"    attr="config"    condition="STM32F1xx LD"/>
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_md.s"    attr="config"    condition="STM32F1xx MD"/>
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_hd.s"    attr="config"    condition="STM32F1xx HD"/>
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_xl.s"    attr="config"    condition="STM32F1xx XL"/>
        <file category="source" name="Device\Source\ARM\startup_stm32f10x_cl.s"    attr="config"    condition="STM32F1xx CL"/>
        
        <!-- system file -->
        <file category="source" name="Device\Source\system_stm32f10x.c"            attr="config"/>
        <!-- device configuration required by drivers at the moment -->
        <file category="header" name="RTE_Driver\Config\RTE_Device.h"              attr="config"/>
      </files>
    </component>

    <!-- START: STMicroelectronics Standard Peripherals Drivers -->

    <component Cclass="Device" Cgroup="StdPeriph Drivers" Csub="Framework" Cversion="3.5.1" condition="STM32F1 STDPERIPH">
      <description>Standard Peripherals Drivers Framework</description>
      <RTE_Components_h>
        #define RTE_DEVICE_STDPERIPH_FRAMEWORK
      </RTE_Components_h>
      <files>
        <file category="doc"     name="Device\StdPeriph_Driver\stm32f10x_stdperiph_lib_um.chm"/>
        <file category="include" name="Device\StdPeriph_Driver\inc\"/>
        <file category="source"  name="Device\StdPeriph_Driver\src\misc.c"/>
        <file category="source"  name="Device\StdPeriph_Driver\templates\stm32f10x_conf.h" attr="config" version="3.5.0"/>
        <file category="include" name="Device\StdPeriph_Driver\templates\stm32f10x_it.h"   attr="template" select="Interrupt Service Routines"/>
        <file category="source"  name="Device\StdPeriph_Driver\templates\stm32f10x_it.c"   attr="template" select="Interrupt Service Routines"/>
      </files>
    </component>

